{
  "name": "throttle-queue",
  "version": "0.1.0",
  "license": "MIT",
  "main": "build/index.js",
  "scripts": {
    "postinstall": "echo \"#!/bin/sh\nyarn run precommit\ngit update-index --add .\" > .git/hooks/pre-commit && chmod a+x .git/hooks/pre-commit",
    "precommit": "yarn run test && yarn run prestart && yarn run document",
    "build": "rm -rf ./build && tsc -p ./tsconfig-src.json",
    "lint": "tslint src/**/*.ts",
    "pretest": "yarn run lint && rm -rf ./test && tsc -p ./tsconfig-test.json",
    "test": "mocha test",
    "document": "rm -rf ./doc && typedoc --tsconfig ./tsconfig-src.json --out ./docs ./src/ && touch ./docs/.nojekyll"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/jest": "^26.0.13",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.4",
    "chai": "^4.2.0",
    "mocha": "^8.1.3",
    "tslib": "^2.0.1",
    "tslint": "^6.1.3",
    "typedoc": "^0.19.1",
    "typescript": "^4.0.2"
  }
}
